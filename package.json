{
    "name": "vscode-glimmer",
    "displayName": "vscode-glimmer",
    "description": "VSCode extension for glimmer",
    "version": "0.0.2",
    "license": "MIT",
    "publisher": "chiragpat",
    "repository": {
        "url": "https://github.com/chiragpat/vscode-glimmer"
    },
    "author": {
        "name": "Chirag Patel"
    },
    "engines": {
        "vscode": "^1.38.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "javascript",
                "extensions": [
                    ".gjs"
                ]
            },
            {
                "id": "typescript",
                "extensions": [
                    ".gts"
                ]
            }
        ],
        "grammars": [
            {
                "injectTo": [
                    "source.js",
                    "source.ts"
                ],
                "scopeName": "inline.hbs",
                "path": "./syntaxes/inline-hbs.json",
                "embeddedLanguages": {
                    "meta.embedded.block.html": "text.html.handlebars"
                }
            },
            {
                "injectTo": [
                    "source.js",
                    "source.ts"
                ],
                "scopeName": "inline.template",
                "path": "./syntaxes/inline-template.json",
                "embeddedLanguages": {
                    "meta.embedded.block.html": "text.html.handlebars"
                }
            }
        ]
    },
    "icon": "icon.png"
}
